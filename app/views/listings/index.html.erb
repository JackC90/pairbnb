<div class="hero-header">
  <div class="main-text">Listings</div>
</div>
<div class="jumbotron">
  <table>
    <tr>
      <th>Location</th>
      <th>Address</th>
      <th>Price</th>
      <th>Description</th>
      <th>Availability</th>
      <th>Maximum Occupants</th>
      <th>Number of Bedrooms</th>
      <th>Number of Bathrooms</th>
      <th colspan=3>Action</th>
    </tr>

    <%= @listings.each do |listing| %>
    <tr>
      <td><%= listing.location %></td>
      <td><%= listing.address %></td>
      <td><%= listing.price %></td>
      <td><%= listing.description %></td>
      <td><%= listing.availability %></td>
      <td><%= listing.max_occupants %></td>
      <td><%= listing.no_of_bedrooms %></td>
      <td><%= listing.no_of_bathrooms %></td>
      <td><%=link_to "Show", listing_path(listing)%></td>
      <td><%= link_to "Edit", edit_listing_path(listing)%></td> 
      <td><%= link_to "Delete", listing_path(listing), method: :delete, data: {confirm: "Are you sure?"} %></td>
    </tr>
    <% end %>
  </table>
</div>

